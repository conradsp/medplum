{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src", "../emr/src/EMRApp.tsx", "../emr/src/Header.tsx", "../emr/src/PatientMainSection.tsx", "../emr/src/PatientSidebar.tsx", "../emr/src/PatientTopMenu.tsx"],
  "exclude": ["dist"]
}
