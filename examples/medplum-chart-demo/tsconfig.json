{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": ["src", "../emr/src/EMRApp.tsx", "../emr/src/components/Header.tsx", "../emr/src/components/PatientMainSection.tsx.old", "../emr/src/components/PatientSidebar.tsx.old", "../emr/src/components/PatientTopMenu.tsx.old"],
  "exclude": ["dist"]
}
